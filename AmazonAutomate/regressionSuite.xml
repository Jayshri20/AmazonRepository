    <classes>
      <class name="TestNGpack.VerifyAddToCardButton"/>
      <class name="TestNGpack.VerifyProductDetailspage"/>
      <class name="TestNGpack.VerifyHomePage"/>
     
    </classes>
  </test> <!-- Test -->
  
  
</suite> <!-- Suite -->
