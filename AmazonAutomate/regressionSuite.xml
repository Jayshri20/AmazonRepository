<suite name="RegressionTest" thread-count="3" parallel="classes">

<listeners>
<listener class-name="extendReport.ExtendTestNGIReporterListener"/>
</listeners>

  <test  name="ChromeTest">
  <parameter name="browser" value="Chrome"/>
    <classes>
      <class name="TestNGpack.VerifyAddToCardButton"/>
      <class name="TestNGpack.VerifyProductDetailspage"/>
      <class name="TestNGpack.VerifyHomePage"/>
     
    </classes>
  </test> <!-- Test -->
  
  <test  name="EdgeTest">
